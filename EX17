/*Exercício 19 página 46*/
#include <stdio.h> 
#include <stdlib.h> 
int main(){ 
    int alturaescada,alturadesejada,qtdedegraus; 
    printf("Qual o tamanho de cada degrau da escada em metros?\n"); 
    scanf("%i",&alturaescada); 
    printf("qual a altura em que deseja chegar? Também em metros \n"); 
    scanf("%i",&alturadesejada); 
    qtdedegraus=alturadesejada/alturaescada; 
    printf("A quantidade de degraus para chegar na altura de %i, é de %i",alturadesejada,qtdedegraus); 
    return 0; 
} 